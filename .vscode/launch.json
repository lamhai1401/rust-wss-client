  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'wss11'",
      "cargo": {
        "args": [
          "build",
          // "--lib",
          "--bin=wssclient",
          "--package=wssclient"
        ],
        "filter": {
          "name": "wssclient",
          "kind": "bin"
        }
      },
      "args": ["--url", "https://localhost/"],
      "cwd": "${workspaceFolder}",
      // "cwd": "${workspaceRoot}/target/debug/",
      "stopOnEntry": false,
      "sourceLanguages": ["rust"],
      // "program": "${cargo:program}",
      "env": {
        "CLIENT_HOST": "http://localhost:8088"
        // "RUST_BACKTRACE": "1"
      }
    }
  ]
}
